{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "module": "esnext",
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es5"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "rootDir": ".",
    "downlevelIteration": true,
    "jsx": "react",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "typeRoots": [
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "*": [
        "node_modules/@types/*",
        "*"
      ]
    }
  },
  "include": [
    "src/**/*"
, "src/pages/dashboard/.tsx"  ]
}